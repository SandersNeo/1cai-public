/// <doc>
/// Применяет интерполяцию токенов {{fake:...}} в строке
/// Пример:
///   М1сAIТестовыеДанные.Apply("Привет, {{fake:firstname ru}}! Email: {{fake:email domain=test.ru}}")
/// </doc>

Функция Apply(ШаблонСтрока, Опции = Неопределено) Экспорт
	Результат = ШаблонСтрока;
	ПозицияНачала = СтрНайти(Результат, "{{fake:");
	Пока ПозицияНачала > 0 Цикл
		ПозицияКонца = СтрНайти(Результат, "}}", ПозицияНачала);
		Если ПозицияКонца > 0 Тогда
			Токен = Сред(Результат, ПозицияНачала, ПозицияКонца - ПозицияНачала + 2);
			КомандаГенератора = Сред(Результат, ПозицияНачала + 7, ПозицияКонца - ПозицияНачала - 8);
			ЗначениеТокена = ВыполнитьТокен(КомандаГенератора);
			Результат = СтрЗаменить(Результат, Токен, ЗначениеТокена);
		Иначе
			Прервать;
		КонецЕсли;
		ПозицияНачала = СтрНайти(Результат, "{{fake:", ПозицияНачала + 1);
	КонецЦикла;
	Возврат Результат;
КонецФункции


Функция ВыполнитьТокен(КомандаГенератора)
	ЧастиКоманды = Split(КомандаГенератора, " ");
	Если ЧастиКоманды.Количество() = 0 Тогда
		Возврат "";
	КонецЕсли;
	
	ИмяГенератора = НРег(СокрЛП(ЧастиКоманды[0]));
	Параметры = Новый Структура;
	
	Для Индекс = 1 По ЧастиКоманды.ВГраница() Цикл
		Парам = СокрЛП(ЧастиКоманды[Индекс]);
		Если СтрНайти(Парам, "=") > 0 Тогда
			ЧастиПарам = Split(Парам, "=");
			Если ЧастиПарам.Количество() = 2 Тогда
				Параметры.Вставить(ЧастиПарам[0], ЧастиПарам[1]);
			КонецЕсли;
		Иначе
			Параметры.Вставить("param" + Строка(Индекс), Парам);
		КонецЕсли;
	КонецЦикла;
	
	Если ИмяГенератора = "firstname" Тогда
		Локаль = Параметры.Получить("param1", Параметры.Получить("locale", Неопределено));
		Пол = Параметры.Получить("param2", Параметры.Получить("gender", Неопределено));
		Возврат FirstName(Локаль, Пол);
	ИначеЕсли ИмяГенератора = "lastname" Тогда
		Локаль = Параметры.Получить("param1", Параметры.Получить("locale", Неопределено));
		Возврат LastName(Локаль);
	ИначеЕсли ИмяГенератора = "fullname" Тогда
		Локаль = Параметры.Получить("param1", Параметры.Получить("locale", Неопределено));
		Пол = Параметры.Получить("param2", Параметры.Получить("gender", Неопределено));
		Возврат FullName(Локаль, Пол);
	ИначеЕсли ИмяГенератора = "email" Тогда
		Домен = Параметры.Получить("domain", Неопределено);
		Возврат Email(Домен);
	ИначеЕсли ИмяГенератора = "phone" Тогда
		Локаль = Параметры.Получить("param1", Параметры.Получить("locale", "ru"));
		Тип = Параметры.Получить("param2", Параметры.Получить("type", "mobile"));
		Возврат Phone(Локаль, Тип);
	ИначеЕсли ИмяГенератора = "inn" Тогда
		Возврат INN();
	ИначеЕсли ИмяГенератора = "kpp" Тогда
		Возврат KPP();
	ИначеЕсли ИмяГенератора = "ogrn" Тогда
		Возврат OGRN();
	ИначеЕсли ИмяГенератора = "price" Тогда
		Мин = Число(Параметры.Получить("param1", "100"));
		Макс = Число(Параметры.Получить("param2", "1000"));
		Валюта = Параметры.Получить("param3", Параметры.Получить("currency", "RUB"));
		Возврат Price(Мин, Макс, Валюта);
	ИначеЕсли ИмяГенератора = "date" Тогда
		r = Параметры.Получить("recent", Неопределено);
		Если r <> Неопределено Тогда
			Возврат Recent(Число(r));
		Иначе
			Возврат Recent();
		КонецЕсли;
	ИначеЕсли ИмяГенератора = "uuidv4" Тогда
		Возврат UUIDv4();
	ИначеЕсли ИмяГенератора = "ulid" Тогда
		Возврат ULID();
	Иначе
		Возврат "{{" + КомандаГенератора + "}}";
	КонецЕсли;
КонецФункции

