/// <doc>ИНН (10 для юр., 12 для физ.)</doc>

Функция INN(ДляФЛ = Ложь) Экспорт
	Если ДляФЛ Тогда
		ОсноваИНН = FromPattern("##########");
		КонтрольнаяСумма1 = ВычислитьКонтрольнуюСуммуИНН(ОсноваИНН, 10);
		КонтрольнаяСумма2 = ВычислитьКонтрольнуюСуммуИНН(ОсноваИНН + КонтрольнаяСумма1, 11);
		Возврат ОсноваИНН + КонтрольнаяСумма1 + КонтрольнаяСумма2;
	Иначе
		ОсноваИНН = FromPattern("#########");
		КонтрольнаяСумма = ВычислитьКонтрольнуюСуммуИНН(ОсноваИНН, 10);
		Возврат ОсноваИНН + КонтрольнаяСумма;
	КонецЕсли;
КонецФункции

/// <doc>КПП</doc>

Функция KPP() Экспорт
	Возврат FromPattern("#########");
КонецФункции

/// <doc>ОГРН/ОГРНИП</doc>

Функция OGRN(ДляИП = Ложь) Экспорт
	Если ДляИП Тогда
		ОсноваОГРН = "3" + FromPattern("##") + FromPattern("####") + FromPattern("######");
		КонтрольнаяЦифра = ВычислитьКонтрольнуюЦифруОГРН(ОсноваОГРН, 15);
		Возврат ОсноваОГРН + КонтрольнаяЦифра;
	Иначе
		МассивЦифр = М1сAIМассивы.Of("1", "2", "5");
		ОсноваОГРН = OneOf(МассивЦифр) + FromPattern("##") + FromPattern("####") + FromPattern("#####");
		КонтрольнаяЦифра = ВычислитьКонтрольнуюЦифруОГРН(ОсноваОГРН, 13);
		Возврат ОсноваОГРН + КонтрольнаяЦифра;
	КонецЕсли;
КонецФункции

#КонецОбласти

