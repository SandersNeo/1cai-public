/// <doc>
/// Генерирует таблицу значений по схеме
/// Параметры:
///   Количество - Число строк
///   Схема - Структура (Ключ=ИмяКолонки, Значение=описание генератора)
/// Пример:
///   Схема = Новый Структура("Имя,Email,Возраст", "FirstName", "Email", "RandomInt");
///   ТаблицаДанных = М1сAIТестовыеДанные.GenerateTable(100, Схема);
/// </doc>

Функция GenerateTable(Количество, Схема) Экспорт
	ТаблицаДанных = Новый ТаблицаЗначений;
	
	Для Каждого КлючЗначение Из Схема Цикл
		ИмяКолонки = КлючЗначение.Ключ;
		Если НЕ ТаблицаДанных.Колонки.Найти(ИмяКолонки) Тогда
			ТаблицаДанных.Колонки.Добавить(ИмяКолонки);
		КонецЕсли;
	КонецЦикла;
	
	Для Индекс = 1 По Количество Цикл
		НоваяСтрока = ТаблицаДанных.Добавить();
		Для Каждого КлючЗначение Из Схема Цикл
			ИмяКолонки = КлючЗначение.Ключ;
			ОписаниеГенератора = КлючЗначение.Значение;
			НоваяСтрока[ИмяКолонки] = ВыполнитьТокен(ОписаниеГенератора);
		КонецЦикла;
	КонецЦикла;
	
	Возврат ТаблицаДанных;
КонецФункции

