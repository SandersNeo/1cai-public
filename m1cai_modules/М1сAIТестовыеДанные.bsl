/// Возвращает "ru"/"en"/"tr" (если не передано — "ru")

Функция ПолучитьТекущуюЛокаль(Локаль)
	Если Локаль <> Неопределено Тогда
		Возврат НРег(Локаль);
	КонецЕсли;
	Возврат "ru";
КонецФункции

/// <doc>Генерирует имя</doc>

Функция FirstName(Локаль = Неопределено, Пол = Неопределено) Экспорт
	ТекЛокаль = ПолучитьТекущуюЛокаль(Локаль);
	
	Если ТекЛокаль = "ru" Тогда
		Если Пол = "male" Тогда
			Возврат OneOf(ПолучитьРусскиеМужскиеИмена());
		ИначеЕсли Пол = "female" Тогда
			Возврат OneOf(ПолучитьРусскиеЖенскиеИмена());
		Иначе
			ВсеИмена = Новый Массив;
			ДополнитьМассив(ВсеИмена, ПолучитьРусскиеМужскиеИмена());
			ДополнитьМассив(ВсеИмена, ПолучитьРусскиеЖенскиеИмена());
			Возврат OneOf(ВсеИмена);
		КонецЕсли;
	ИначеЕсли ТекЛокаль = "en" Тогда
		Возврат OneOf(ПолучитьАнглийскиеИмена());
	Иначе
		Возврат OneOf(ПолучитьРусскиеМужскиеИмена());
	КонецЕсли;
КонецФункции

/// <doc>Генерирует фамилию</doc>

Функция LastName(Локаль = Неопределено, Пол = Неопределено) Экспорт
	ТекЛокаль = ПолучитьТекущуюЛокаль(Локаль);
	
	Если ТекЛокаль = "ru" Тогда
		Возврат OneOf(ПолучитьРусскиеФамилии());
	ИначеЕсли ТекЛокаль = "en" Тогда
		Возврат OneOf(ПолучитьАнглийскиеФамилии());
	Иначе
		Возврат OneOf(ПолучитьРусскиеФамилии());
	КонецЕсли;
КонецФункции

/// <doc>Полное имя (Имя Фамилия)</doc>

Функция FullName(Локаль = Неопределено, Пол = Неопределено) Экспорт
	Имя = FirstName(Локаль, Пол);
	Фамилия = LastName(Локаль, Пол);
	Возврат СтрШаблон("%1 %2", Имя, Фамилия);
КонецФункции

