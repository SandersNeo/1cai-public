/// <doc>Генерирует email</doc>

Функция Email(Домен = Неопределено) Экспорт
	ИмяПользователя = НРег(СтрЗаменить(FirstName("en"), " ", ""));
	ИмяПользователя = ИмяПользователя + Строка(RandomInt(1, 999));
	Если Домен = Неопределено Тогда
		Домен = OneOf(ПолучитьПопулярныеДомены());
	КонецЕсли;
	Возврат СтрШаблон("%1@%2", ИмяПользователя, Домен);
КонецФункции

/// <doc>Генерирует доменное имя</doc>

Функция Domain() Экспорт
	Слово = НРег(FirstName("en"));
	Зона = OneOf(ПолучитьДоменныеЗоны());
	Возврат СтрШаблон("%1.%2", Слово, Зона);
КонецФункции

/// <doc>Генерирует URL</doc>

Функция URL(ГлубинаПути = 0) Экспорт
	Протокол = OneOf(ПолучитьПротоколы());
	Домен = Domain();
	
	Если ГлубинаПути = 0 Тогда
		Возврат СтрШаблон("%1://%2", Протокол, Домен);
	КонецЕсли;
	
	Путь = "";
	Для Индекс = 1 По ГлубинаПути Цикл
		Сегмент = НРег(FirstName("en"));
		Путь = Путь + "/" + Сегмент;
	КонецЦикла;
	
	Возврат СтрШаблон("%1://%2%3", Протокол, Домен, Путь);
КонецФункции

