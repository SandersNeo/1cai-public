/// <doc>Генерирует номер телефона по локали</doc>

Функция Phone(Локаль = "ru", Тип = "mobile") Экспорт
	Если Локаль = "ru" Тогда
		Если Тип = "mobile" Тогда
			Код = OneOf(ПолучитьМобильныеКоды());
			Номер = FromPattern("###-##-##");
			Возврат СтрШаблон("+7 (%1) %2", Код, Номер);
		Иначе
			КодГорода = OneOf(ПолучитьКодыГородов());
			Номер = FromPattern("##-##-##");
			Возврат СтрШаблон("+7 (%1) %2", КодГорода, Номер);
		КонецЕсли;
	ИначеЕсли Локаль = "en" Тогда
		Возврат "+1 " + FromPattern("(###) ###-####");
	Иначе
		Возврат "+7 " + FromPattern("(9##) ###-##-##");
	КонецЕсли;
КонецФункции

