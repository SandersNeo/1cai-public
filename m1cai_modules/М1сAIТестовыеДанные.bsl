/// <doc>Номер карты (Луна по желанию)</doc>

Функция CardNumber(Бренд = "visa", ВалидныйЛуна = Истина) Экспорт
	Если Бренд = "visa" Тогда
		Основа = "4" + FromPattern("###############");
	ИначеЕсли Бренд = "mastercard" Тогда
		Основа = "5" + Строка(RandomInt(1, 5)) + FromPattern("##############");
	ИначеЕсли Бренд = "mir" Тогда
		Основа = "220" + FromPattern("#############");
	Иначе
		Основа = FromPattern("################");
	КонецЕсли;
	
	Если ВалидныйЛуна Тогда
		КонтрольнаяЦифра = ВычислитьКонтрольнуюЦифруЛуна(Лев(Основа, 15));
		Возврат Лев(Основа, 15) + КонтрольнаяЦифра;
	Иначе
		Возврат Основа;
	КонецЕсли;
КонецФункции

