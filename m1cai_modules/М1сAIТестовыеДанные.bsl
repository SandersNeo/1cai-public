Функция ВычислитьКонтрольнуюСуммуИНН(ОсноваИНН, ДлинаИНН)
	Коэффициенты10 = М1сAIМассивы.Of(2, 4, 10, 3, 5, 9, 4, 6, 8);
	Коэффициенты11 = М1сAIМассивы.Of(7, 2, 4, 10, 3, 5, 9, 4, 6, 8);
	
	Сумма = 0;
	Коэффициенты = ?(ДлинаИНН = 10, Коэффициенты10, Коэффициенты11);
	
	Для Индекс = 0 По Коэффициенты.ВГраница() Цикл
		Цифра = Число(Сред(ОсноваИНН, Индекс + 1, 1));
		Сумма = Сумма + Цифра * Коэффициенты[Индекс];
	КонецЦикла;
	
	Возврат Строка(Сумма % 11 % 10);
КонецФункции

Функция ВычислитьКонтрольнуюЦифруОГРН(ОсноваОГРН, ДлинаОГРН)
